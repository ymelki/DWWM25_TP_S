{% extends 'base.html.twig' %}

{% block title %}ProfileCategorie index{% endblock %}

{% block body %}
    <h1>ProfileCategorie index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for profile_categorie in profile_categories %}
            <tr>
                <td>{{ profile_categorie.id }}</td>
                <td>{{ profile_categorie.nom }}</td>
                <td>
                    <a href="{{ path('app_profile_categorie_show', {'id': profile_categorie.id}) }}">show</a>
                    <a href="{{ path('app_profile_categorie_edit', {'id': profile_categorie.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_profile_categorie_new') }}">Create new</a>
{% endblock %}
