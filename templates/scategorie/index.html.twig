{% extends 'base.html.twig' %}

{% block title %}Scategorie index{% endblock %}

{% block body %}
    <h1>Scategorie index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for scategorie in scategories %}
            <tr>
                <td>{{ scategorie.id }}</td>
                <td>{{ scategorie.nom }}</td>
                <td>
                    <a href="{{ path('app_scategorie_show', {'id': scategorie.id}) }}">show</a>
                 </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

 {% endblock %}
