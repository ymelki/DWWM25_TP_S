{% extends 'base.html.twig' %}

{% block title %}Wcategorie index{% endblock %}

{% block body %}
    <h1>Wcategorie index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for wcategorie in wcategories %}
            <tr>
                <td>{{ wcategorie.id }}</td>
                <td>{{ wcategorie.nom }}</td>
                <td>
                    <a href="{{ path('app_wcategorie_show', {'id': wcategorie.id}) }}">show</a>
                    <a href="{{ path('app_wcategorie_edit', {'id': wcategorie.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_wcategorie_new') }}">Create new</a>
{% endblock %}
